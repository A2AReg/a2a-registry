# A2A Agent Registry Test Configuration

# Application
APP_NAME="A2A Agent Registry Test"
APP_VERSION="1.0.0"
DEBUG=true
TEST_MODE=true

# Database
DATABASE_URL="sqlite:///test.db"

# Redis
REDIS_URL="redis://localhost:6379/1"

# Elasticsearch
ELASTICSEARCH_URL="http://localhost:9200"
ELASTICSEARCH_INDEX="a2a_agents_test"

# Security
SECRET_KEY="test-secret-key-for-testing-only"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30

# OAuth2
OAUTH2_CLIENT_ID="a2a-registry-test"
OAUTH2_CLIENT_SECRET="test-oauth2-secret"
OAUTH2_TOKEN_URL="http://localhost:8000/oauth/token"

# Registry
REGISTRY_BASE_URL="http://localhost:8000"
MAX_AGENTS_PER_CLIENT=1000
SEARCH_TIMEOUT_SECONDS=30

# Federation
ENABLE_FEDERATION=false
MAX_PEER_REGISTRIES=10
PEER_SYNC_INTERVAL_MINUTES=60
