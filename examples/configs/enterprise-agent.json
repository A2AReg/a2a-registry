  "agent_card": {
    "name": "enterprise-analytics-agent",
    "description": "Enterprise-grade analytics agent with advanced security and compliance features",
    "version": "3.2.1",
    "author": "Enterprise AI Solutions Ltd",
    "api_base_url": "https://secure-api.enterprise-ai.com/v3",
    
    "capabilities": {
      "protocols": ["https", "wss", "grpc"],
      "supported_formats": ["json", "xml", "csv", "parquet"],
      "max_request_size": 104857600,
      "max_concurrent_requests": 100,
      "a2a_version": "1.0"
    },
    
    "auth_schemes": [
      {
        "type": "oauth2",
        "description": "OAuth 2.0 with PKCE for secure authentication",
        "required": true
      },
      {
        "type": "mtls", 
        "description": "Mutual TLS for certificate-based authentication",
        "required": false
      },
      {
        "type": "jwt",
        "description": "JSON Web Tokens for stateless authentication", 
        "required": false,
        "header_name": "Authorization"
      }
    ],
    
    "endpoints": {
      "analyze": "/analyze",
      "report": "/report",
      "dashboard": "/dashboard",
      "export": "/export",
      "health": "/health",
      "metrics": "/metrics",
      "audit": "/audit",
      "compliance": "/compliance"
    },
    
    "skills": {
      "input_schema": {
        "type": "object",
        "properties": {
          "data_source": {
            "type": "object",
            "properties": {
              "type": {"type": "string", "enum": ["database", "file", "api", "stream"]},
              "connection": {"type": "string"},
              "query": {"type": "string"}
            },
            "required": ["type"]
          },
          "analysis_type": {
            "type": "string",
            "enum": ["descriptive", "diagnostic", "predictive", "prescriptive"]
          },
          "parameters": {
            "type": "object",
            "properties": {
              "time_range": {
                "type": "object",
                "properties": {
                  "start": {"type": "string", "format": "date-time"},
                  "end": {"type": "string", "format": "date-time"}
                }
              },
              "dimensions": {"type": "array", "items": {"type": "string"}},
              "metrics": {"type": "array", "items": {"type": "string"}},
              "filters": {"type": "object"}
            }
          },
          "output_format": {
            "type": "string",
            "enum": ["json", "csv", "parquet", "dashboard"],
            "default": "json"
          }
        },
        "required": ["data_source", "analysis_type"]
      },
      
      "output_schema": {
        "type": "object",
        "properties": {
          "analysis_id": {"type": "string"},
          "results": {
            "type": "object",
            "properties": {
              "summary": {"type": "object"},
              "data": {"type": "array"},
              "insights": {"type": "array", "items": {"type": "string"}},
              "recommendations": {"type": "array", "items": {"type": "string"}}
            }
          },
          "metadata": {
            "type": "object",
            "properties": {
              "processing_time_ms": {"type": "integer"},
              "data_points_analyzed": {"type": "integer"},
              "confidence_score": {"type": "number", "minimum": 0, "maximum": 1},
              "compliance_status": {"type": "string", "enum": ["compliant", "non-compliant", "pending"]}
            }
          },
          "export_urls": {
            "type": "object",
            "properties": {
              "dashboard": {"type": "string"},
              "report_pdf": {"type": "string"},
              "data_csv": {"type": "string"}
            }
          }
        },
        "required": ["analysis_id", "results", "metadata"]
      },
      
      "examples": [
        "Analyze sales data: {'data_source': {'type': 'database', 'query': 'SELECT * FROM sales'}, 'analysis_type': 'descriptive'} -> Comprehensive sales analytics with trends and insights",
        "Predictive analysis: {'data_source': {'type': 'api'}, 'analysis_type': 'predictive', 'parameters': {'time_range': {'start': '2024-01-01', 'end': '2024-12-31'}}} -> Future trends and forecasts",
        "Compliance report: {'data_source': {'type': 'database'}, 'analysis_type': 'diagnostic', 'output_format': 'dashboard'} -> GDPR/SOX compliance dashboard"
      ]
    }
  }
}
