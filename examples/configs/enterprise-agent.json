{
  "name": "Enterprise Analytics Agent",
  "description": "Enterprise-grade analytics agent with advanced security and compliance features for business intelligence and data analysis",
  "url": "https://secure-api.enterprise-ai.com/v3/analytics-agent",
  "provider": {
    "organization": "Enterprise AI Solutions Ltd",
    "url": "https://enterprise-ai-solutions.com"
  },
  "version": "3.2.1",
  "documentationUrl": "https://secure-api.enterprise-ai.com/v3/docs",
  "capabilities": {
    "streaming": true,
    "pushNotifications": true,
    "stateTransitionHistory": true,
    "supportsAuthenticatedExtendedCard": true
  },
  "securitySchemes": [
    {
      "type": "oauth2",
      "flow": "client_credentials",
      "tokenUrl": "https://enterprise-ai.com/oauth/token",
      "scopes": ["analytics", "reports", "data", "admin"]
    },
    {
      "type": "mTLS",
      "location": "certificate",
      "credentials": "enterprise_jwt_token_or_certificate"
    },
    {
      "type": "apiKey",
      "location": "header",
      "name": "X-Enterprise-Key"
    }
  ],
  "interface": {
    "preferredTransport": "grpc",
    "additionalInterfaces": [
      {
        "transport": "jsonrpc",
        "url": "https://secure-api.enterprise-ai.com/v3/jsonrpc"
      },
      {
        "transport": "http",
        "url": "https://secure-api.enterprise-ai.com/v3/api"
      }
    ],
    "defaultInputModes": ["application/json", "multipart/form-data"],
    "defaultOutputModes": ["application/json", "application/csv", "application/pdf"]
  },
  "skills": [
    {
      "id": "data_analysis",
      "name": "Data Analysis",
      "description": "Perform comprehensive data analysis including descriptive, diagnostic, predictive, and prescriptive analytics",
      "tags": ["analytics", "data", "business-intelligence", "statistics"],
      "examples": [
        "Analyze sales data trends for Q4",
        "Perform predictive analysis on customer churn",
        "Generate descriptive statistics for marketing campaigns"
      ],
      "inputModes": ["application/json"],
      "outputModes": ["application/json", "application/csv"]
    },
    {
      "id": "dashboard_generation",
      "name": "Dashboard Generation",
      "description": "Create interactive dashboards and visualizations for business metrics",
      "tags": ["dashboard", "visualization", "reporting", "business-intelligence"],
      "examples": [
        "Create executive dashboard for KPIs",
        "Generate sales performance dashboard",
        "Build customer analytics visualization"
      ],
      "inputModes": ["application/json"],
      "outputModes": ["application/json", "text/html"]
    },
    {
      "id": "compliance_reporting",
      "name": "Compliance Reporting",
      "description": "Generate compliance reports for GDPR, SOX, and other regulatory requirements",
      "tags": ["compliance", "reporting", "gdpr", "sox", "regulatory"],
      "examples": [
        "Generate GDPR compliance report",
        "Create SOX audit documentation",
        "Prepare regulatory filing data"
      ],
      "inputModes": ["application/json"],
      "outputModes": ["application/pdf", "application/json"]
    },
    {
      "id": "predictive_modeling",
      "name": "Predictive Modeling",
      "description": "Build and deploy predictive models for forecasting and trend analysis",
      "tags": ["predictive", "modeling", "forecasting", "machine-learning"],
      "examples": [
        "Predict customer lifetime value",
        "Forecast sales for next quarter",
        "Model risk assessment for investments"
      ],
      "inputModes": ["application/json"],
      "outputModes": ["application/json"]
    },
    {
      "id": "data_export",
      "name": "Data Export",
      "description": "Export analyzed data in various formats for external systems and reporting",
      "tags": ["export", "data", "integration", "reporting"],
      "examples": [
        "Export analysis results to CSV",
        "Generate PDF report for stakeholders",
        "Create API endpoint for data access"
      ],
      "inputModes": ["application/json"],
      "outputModes": ["application/csv", "application/pdf", "application/json"]
    }
  ]
}
